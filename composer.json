{
  "name": "acelot/graphql",
  "description": "GraphQL query resolver for PHP 7",
  "keywords": [
    "graphql",
    "graphql-server",
    "api"
  ],
  "type": "library",
  "homepage": "https://github.com/acelot/graphql",
  "license": "MIT",
  "authors": [
    {
      "name": "Valeriy Protopopov",
      "email": "provaleriy@gmail.com"
    }
  ],
  "scripts": {
    "unit": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Acelot\\GraphQL\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Acelot\\GraphQL\\Tests\\Unit\\": "tests/unit",
      "Acelot\\GraphQL\\Tests\\Functional\\": "tests/functional"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-parle": "^0.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.4",
    "phpstan/phpstan": "^0.11.2"
  }
}
